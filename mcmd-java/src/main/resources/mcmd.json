{
	"config": {
		"maxWait": 1000,
		"intervalBtw": 1000,
		"intervalWait": 2000
	},
	"mcmd0": {
		"hostInfo": {
			"host": "topzone.biz",
			"port": 22,
			"username": "ubuntu",
			"keyfile": "/Users/dhong/.ssh/topzone_ca1.pem"
		},
		"work1": {
			"type": "ssh",
			"commands": [
				"cd /home/ubuntu",
				"ls -al",
				"_SLEEP 3",
				"_FINISH"
			]
		}
	},
	"mcmd1": {
		"hostInfo": {
			"host": "topzone.biz",
			"port": 22,
			"username": "ubuntu",
			"keyfile": "/Users/dhong/.ssh/topzone_ca1.pem"
		},
		"work1": {
			"type": "ssh",
			"commands": [
				"cd /home/ubuntu",
				"rm -Rf {{filename}}.tar",
				"rm -Rf {{filename}}.tar_bak",
				"tar cvf {{filename}}.tar *",
				"_WAIT ':~$",
				"_SLEEP 3",
				"mv {{filename}}.tar {{filename}}.tar_bak",
				"_FINISH"
			]
		},
		"work2": {
			"type": "shell",
			"commands": [
				"scp -i {{keyfile}} {{username}}@{{host}}:/home/ubuntu/{{filename}}.tar_bak ."
			]
		}
	},
	"kali_aws": {
		"hostInfo": {
			"host": "54.183.204.226",
			"port": 22,
			"username": "ec2-user",
			"keyfile": "/Users/dhong/.ssh/topzone_ca1.pem"
		},
		"work1": {
			"type": "ssh",
			"commands": [
				"cd /opt/discover",
				"sudo bash discover.sh",
				"_WAIT 'Choice:'",
				"1",
				"_WAIT 'Choice:'",
				"1",
				"_WAIT 'Company:'",
				"{{company}}",
				"_WAIT 'Domain:'",
				"{{domain}}",
				"_WAIT 'Scan complete.'",
				"_FINISH"
			]
		},
		"work2": {
			"type": "ssh",
			"commands": [
				"sudo sqlite3 /root/.recon-ng/workspaces/{{domain}}/data.db .dump .quit > /home/ec2-user/{{company}}.sql",
				"_WAIT ':~$",
				"_FINISH"
			]
		},
		"work3": {
			"type": "shell",
			"commands": [
				"scp -i {{keyfile}} {{username}}@{{host}}:/home/ec2-user/{{company}}.sql .",
				"_SLEEP 10",
				"_REPLACE \"{{company}}.sql\" \"PRAGMA foreign_keys=OFF;\" \"\" ",
				"_REPLACE \"{{company}}.sql\" \"BEGIN TRANSACTION;\" \"\" ",
				"_REPLACE \"{{company}}.sql\" \"\\\"\" \"\" ",
				"_REPLACE \"{{company}}.sql\" \" PRIMARY KEY\" \"\" ",
				"_REPLACE \"{{company}}.sql\" \"COMMIT;\" \"\" ",
				"_PREFIX \"{{company}}.sql\" \" create database IF NOT EXISTS {{company}}; use {{company}}; \" ",
				"/usr/local/bin/mysql --user={{user}} --password={{password}} -h {{mysql_host}} < {{_curdir}}/{{company}}.sql "
			]
		}
	}
}